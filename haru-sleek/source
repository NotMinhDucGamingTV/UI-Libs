			DropdownFrame.Size = UDim2.new(0, 225, 0, 25)

			UICorner_3.Parent = DropdownFrame

			ImageLabel.Parent = DropdownFrame
			ImageLabel.AnchorPoint = Vector2.new(0.5, 0.5)
			ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
